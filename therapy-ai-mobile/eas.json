{
  "cli": {
    "version": ">= 16.27.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "env": {
        "EXPO_NO_TYPESCRIPT_CHECK": "1"
      },
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "image": "latest"
      }
    }
  }
}
